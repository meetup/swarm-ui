{
  "name": "@meetup/swarm-styles",
  "version": "0.13.6",
  "description": "Swarm UI styles",
  "author": "hsbacot <hsbacot@gmail.com>",
  "homepage": "",
  "license": "ISC",
  "main": "lib/styles.js",
  "directories": {
    "dist": "dist",
    "test": "__tests__"
  },
  "files": [
    "lib",
    "dist"
  ],
  "scripts": {
    "build": "yarn postcss",
    "develop": "chokidar \"lib/**/*.css\" -c \"postcss lib/main.css -o dist/main.css && postcss lib/global.css -o dist/global.css\"",
    "postcss": "postcss lib/main.css -o dist/main.css && postcss lib/global.css -o dist/global.css"
  },
  "devDependencies": {
    "autoprefixer": "9.5.0",
    "chokidar-cli": "1.2.2",
    "cssnano": "4.1.10",
    "postcss-cli": "6.1.2",
    "postcss-extend-rule": "2.0.0",
    "postcss-import": "12.0.1"
  },
  "dependencies": {
    "@meetup/swarm-constants": "^0.9.4",
    "@meetup/swarm-icons": "^0.5.1",
    "@reach/menu-button": "^0.1.18"
  }
}
